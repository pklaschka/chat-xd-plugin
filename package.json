{
	"name": "document-chat-for-adobe-xd",
	"version": "0.4.0",
	"private": true,
	"description": "Chat for XD Cloud Documents",
	"license": "MIT",
	"author": "Pablo Klaschka",
	"scripts": {
		"build": "npm run compile && xdpm package dist",
		"compile": "webpack --config webpack.config.js --mode production",
		"debug:install": "webpack --config webpack.config.js --mode production && xdpm install dist -o",
		"debug:watch": "xdpm watch dist | webpack --config webpack.config.js --mode production --watch",
		"debug:watch-prerelease": "xdpm watch dist -w p | webpack --config webpack.config.js --mode production --watch",
		"docs": "jsdoc -r src -d docs",
		"lint": "eslint src/**/*.js --ignore-pattern node_modules/",
		"prettier:check": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"start": "npm run debug:watch",
		"stats": "sloc src",
		"test:e2e": "cypress run",
		"version": "node devtools/sync-versions.js"
	},
	"keywords": [
		"chat",
		"privacy",
		"cloud documents",
		"coediting",
		"collaboration"
	],
	"dependencies": {
		"@adobexd/typings": "github:AdobeXD/typings",
		"@fliegwerk/logsemts": "^0.4.0",
		"@formatjs/intl-relativetimeformat": "^8.0.4",
		"babel-core": "^6.26.3",
		"babel-loader": "^8.2.2",
		"babel-plugin-transform-react-jsx": "^6.24.1",
		"install": "^0.13.0",
		"markdown-it": "^12.0.4",
		"markdown-it-emoji": "^2.0.0",
		"npm": "^7.5.3",
		"pure-md5": "^0.1.13",
		"react": "^16.14.0",
		"react-dom": "^16.14.0",
		"react-intl": "^5.12.2",
		"react-router-dom": "^5.2.0",
		"v4-uuid": "^1.1.0",
		"xd-storage-helper": "^1.2.3"
	},
	"devDependencies": {
		"@adobe/xdpm": "^4.1.1",
		"@types/adobe-xd": "AdobeXD/typings",
		"@types/markdown-it": "^12.0.1",
		"@types/markdown-it-emoji": "^2.0.0",
		"@types/node": "^14.14.26",
		"@types/punycode": "^2.1.0",
		"@types/react": "^17.0.1",
		"@types/react-dom": "^17.0.0",
		"@types/react-intl": "^3.0.0",
		"@types/react-router-dom": "^5.1.7",
		"babel-core": "^6.26.3",
		"babel-loader": "^8.2.2",
		"babel-plugin-transform-react-jsx": "^6.24.1",
		"chalk": "^4.1.0",
		"css-loader": "^5.0.2",
		"cypress": "^6.4.0",
		"cypress-react-unit-test": "^4.17.2",
		"eslint": "^7.19.0",
		"eslint-plugin-prettier": "^3.3.1",
		"file-loader": "^6.2.0",
		"jsdoc": "^3.6.6",
		"postcss-scss": "^3.0.4",
		"prettier": "^2.2.1",
		"prettier-plugin-organize-imports": "^1.1.1",
		"prettier-plugin-package": "^1.3.0",
		"prettier-plugin-style-order": "^0.2.2",
		"sass": "^1.32.7",
		"sass-loader": "^10.1.1",
		"shelljs": "^0.8.4",
		"sloc": "^0.2.1",
		"source-map-loader": "^1.1.3",
		"style-loader": "^2.0.0",
		"typescript": "^4.1.5",
		"ts-loader": "^8.0.17",
		"webpack": "^4.46.0",
		"webpack-cli": "^4.5.0"
	}
}
